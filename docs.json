{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Play Hosting",
  "colors": {
    "primary": "#D2B361",
    "light": "#FAD77B",
    "dark": "#C6A13F"
  },
  "background": {
    "color": {
      "dark": "#121212"
    }
  },
  "logo": {
    "light": "/assets/logo_dark.svg",
    "dark": "/assets/logo_light.svg"
  },
  "favicon": "/assets/favicon.svg",
  "seo": {
    "metatags": {
      "og:image": "/assets/banner.png"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Get started",
            "pages": [
              "introduction",
              "hardware"
            ]
          },
          {
            "group": "Using your server",
            "pages": [
              "minecraft/setup",
              "minecraft/queue",
              "minecraft/console",
              "minecraft/upload"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://play.hosting/discord",
          "icon": "discord"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Create account",
        "href": "https://panel.play.hosting/auth/register"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://panel.play.hosting"
    }
